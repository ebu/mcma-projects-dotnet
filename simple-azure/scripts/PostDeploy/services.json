[
    {
        "@type": "Service",
        "name": "Service Registry",
        "resources": [
            {
                "@type": "ResourceEndpoint",
                "resourceType": "Service",
                "httpEndpoint": "{{{service_registry_url}}}services"
            },
            {
                "@type": "ResourceEndpoint",
                "resourceType": "JobProfile",
                "httpEndpoint": "{{{service_registry_url}}}job-profiles"
            }
        ],
        "authType": "AzureAD",
        "authContext": "{ \"scope\": \"{{{service_registry_url}}}.default\" }"
    },
    {
        "@type": "Service",
        "name": "Job Processor Service",
        "resources": [
            {
                "@type": "ResourceEndpoint",
                "resourceType": "AmeJob",
                "httpEndpoint": "{{{job_processor_url}}}jobs"
            },
            {
                "@type": "ResourceEndpoint",
				"resourceType": "AIJob",
				"httpEndpoint": "{{{job_processor_url}}}jobs"
            },
            {
                "@type": "ResourceEndpoint",
				"resourceType": "CaptureJob",
				"httpEndpoint": "{{{job_processor_url}}}jobs"
            },
            {
                "@type": "ResourceEndpoint",
				"resourceType": "QAJob",
				"httpEndpoint": "{{{job_processor_url}}}jobs"
            },
            {
                "@type": "ResourceEndpoint",
				"resourceType": "TransferJob",
				"httpEndpoint": "{{{job_processor_url}}}jobs"
            },
            {
                "@type": "ResourceEndpoint",
				"resourceType": "TransformJob",
				"httpEndpoint": "{{{job_processor_url}}}jobs"
            },
            {
                "@type": "ResourceEndpoint",
                "resourceType": "WorkflowJob",
                "httpEndpoint": "{{{job_processor_url}}}jobs"
            }
        ],
        "authType": "AzureAD",
        "authContext": "{ \"scope\": \"{{{job_processor_url}}}.default\" }"
    },
    {
        "@type": "Service",
        "name": "FFmpeg Transform Service",
        "resources": [
            {
                "@type": "ResourceEndpoint",
                "resourceType": "JobAssignment",
                "httpEndpoint": "{{{ffmpeg_service_url}}}job-assignments"
            }
        ],
        "jobType": "TransformJob",
        "jobProfileIds": [
            "ExtractThumbnail"
        ],
        "authType": "AzureAD",
        "authContext": "{ \"scope\": \"{{{ffmpeg_service_url}}}.default\" }"
    },
    {
        "@type": "Service",
        "name": "MediaInfo AME Service",
        "resources": [
            {
                "@type": "ResourceEndpoint",
                "resourceType": "JobAssignment",
                "httpEndpoint": "{{{mediainfo_service_url}}}job-assignments"
            }
        ],
        "jobType": "AmeJob",
        "jobProfileIds": [
            "ExtractTechnicalMetadata"
        ],
        "authType": "AzureAD",
        "authContext": "{ \"scope\": \"{{{mediainfo_service_url}}}.default\" }"
    }
]